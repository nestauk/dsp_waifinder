{
	"author": "nestauk (https://www.nesta.org.uk/)",
	"dependencies": {
		"@mapbox/geo-viewport": "^0.4.1",
		"@svizzle/dom": "^0.7.0",
		"@svizzle/geo": "^0.9.0",
		"@svizzle/ui": "^0.7.1",
		"@svizzle/utils": "^0.18.0",
		"@turf/bbox": "^6.5.0",
		"d3-color": "^3.1.0",
		"d3-shape": "^3.1.0",
		"email-regex": "^5.0.0",
		"escape-string-regexp": "^5.0.0",
		"just-compare": "^2.2.2",
		"lamb": "^0.60.0",
		"mapbox-gl": "^2.10.0",
		"supercluster": "^7.1.5",
		"yootils": "^0.3.1"
	},
	"description": "App UI",
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-netlify": "^1.0.0-next.76",
		"@sveltejs/kit": "next",
		"@svizzle/dev": "^0.6.0",
		"@svizzle/file": "^0.14.0",
		"bowser": "^2.11.0",
		"browserstack-capabilities": "^0.7.0",
		"browserstack-local": "^1.5.1",
		"cpy-cli": "^4.2.0",
		"eslint": "^8.23.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"lighthouse": "^9.6.6",
		"mdsvex": "^0.10.6",
		"mkdirp": "^1.0.4",
		"mocha": "^10.0.0",
		"node-fetch": "^3.2.10",
		"object-hash": "^3.0.0",
		"pa11y": "^6.2.3",
		"pa11y-reporter-html": "^2.0.0",
		"queue-promise": "^2.2.1",
		"selenium-webdriver": "^4.4.0",
		"svelte": "^3.50.1",
		"unist-util-visit": "^4.1.1",
		"vite": "^3.1.0"
	},
	"license": "MIT",
	"name": "ai_map_ui",
	"scripts": {
		"build": "vite build",
		"chmod": "chmod u+x src/bin/*",
		"copymapboxcss": "cpy --flat ./node_modules/mapbox-gl/dist/mapbox-gl.css ./static/css/",
		"deploy": "npm run makedata && npm run export",
		"dev": "vite dev",
		"lighthouse": "node test/lighthouse/validate.js",
		"linksvizzles": "npm link @svizzle/dev @svizzle/dom @svizzle/file @svizzle/geo @svizzle/ui @svizzle/utils && npm list",
		"lint": "eslint .",
		"makedata": "npm run makedatadir && npm run mergeAnnotations",
		"makedatadir": "mkdirp static/data",
		"mergeAnnotations": "node src/bin/mergeAnnotations.js",
		"pa11y": "node test/pa11y/validate.js",
		"postinstall": "npm run copymapboxcss",
		"preview": "vite preview",
		"reportToCSV": "npm run chmod && node src/bin/reportToCSV.js",
		"rmdata": "rimraf static/data",
		"selenium": "node test/browserstack/selenium-runner.mjs",
		"unittest": "mocha --spec src/**/*.spec.js"
	},
	"type": "module"
}
