{
  "dependencies": {
    "@svizzle/dev": "^0.6.1",
    "@svizzle/file": "^0.14.1",
    "@svizzle/utils": "^0.19.0",
    "cli-progress": "^3.11.2",
    "dap_dv_backends_utils": "github:nestauk/dap_dv_backends_utils#v0.0.15",
    "lamb": "^0.61.0",
    "mkdirp": "^2.1.3",
    "object-hash": "^3.0.0",
    "undici": "^5.19.1"
  },
  "description": "Backend scripts for filling missing data etc.",
  "scripts": {
    "annotateData": "npx annotateEsIndex -d es.annotations.dap-tools.uk -i ai_map -f description",
    "downloadAnnotations": "node bin/downloadAnnotatedOrgs.js",
    "fillMissingData": "node bin/fillMissingData.js",
    "fillMissingLocations": "node bin/fillMissingLocations.js",
    "getEntityDataQuality": "npx entitiesDataQuality -d es.annotations.dap-tools.uk -i ai_map -o data",
    "getAnnotationsDataQuality": "npx annotationsDataQuality -d es.annotations.dap-tools.uk -i ai_map -o data/quality/annotations",
    "ingestToEs": "npx jsonToEsIndex -d es.annotations.dap-tools.uk -i ai_map -p ../ds/outputs/data/ai_map_orgs_places_refreshed_part_2.json --list-key orgs --batch-size 100",
    "prepareFrontEndData": "node bin/prepareFrontEndData.js"
  },
  "type": "module"
}
