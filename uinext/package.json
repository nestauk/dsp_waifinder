	"license": "MIT",
	"main": "index.js",
	"name": "ai_map_ui",
	"scripts": {
		"build": "npm run rmsappers && sapper build --legacy",
		"chmod": "chmod u+x src/bin/*",
		"copydata": "npm run chmod && node -r esm src/bin/copy_data.js",
		"copymapboxcss": "cpy --flat ./node_modules/mapbox-gl/dist/mapbox-gl.css ./static/css/",
		"deploy": "npm run makedata && npm run export",
		"dev": "npm run rmsappers && sapper dev",
		"export": "npm run rmsappers && export SAPPER_EXPORT=true && sapper export --legacy",
		"lighthouse": "node -r esm test/lighthouse/validate.js",
		"linksvizzles": "npm link @svizzle/ui @svizzle/dev @svizzle/file @svizzle/utils && npm list",
		"lint": "eslint rollup.config.js 'src/**/*.{js,svelte}'",
		"makedata": "npm run makeindex && npm run rmdata && npm run copydata",
		"makeindex": "npm run chmod && node -r esm src/bin/make_index.js",
		"pa11y": "node -r esm test/pa11y/validate.js",
		"reportToCSV": "npm run chmod && node -r esm src/bin/reportToCSV.js",
		"rmdata": "rimraf static/data",
		"rmsappers": "rimraf __sapper__ src/node_modules/@sapper",
		"start": "node __sapper__/build",
		"test": "run-p --race dev cy:run",
		"selenium": "node test/browserstack/selenium-runner.mjs",
		"unittest": "mocha -r esm --spec src/**/*.spec.js"
	}
}
